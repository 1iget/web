RedirectMatch 404 .*/\.git/.*

RewriteEngine on

Options +ExecCGI +FollowSymLinks

<Files *.md5>
ForceType application/binary
</Files>

<Files *.sha1>
ForceType application/binary
</Files>

<Files *.asc>
ForceType application/binary
</Files>

<Files favicon.ico>
ForceType image/vnd.microsoft.icon
</Files>
